{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len":"off",
        "require-jsdoc":"off",
        "camelcase":"off",
        "linebreak-style": "off",
        "eol-last": "off",
        "no-trailing-spaces": "off",
        "indent": "off",
        "object-curly-spacing":"off",
        "no-unused-vars":"warn"
    }
    
}
